<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Equipo">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Cascos de boxeo"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:layout_editor_absoluteX="132dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Guantes de boxeo"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:layout_editor_absoluteX="125dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="98dp"
        android:layout_height="98dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:srcCompat="@drawable/guantes_de_boxeo_100_rojo_boxeadores_nivel_iniciacion_hombre_o_mujer"
        tools:layout_editor_absoluteY="151dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Equipamiento de boxeo"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/include"
        tools:layout_editor_absoluteX="80dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="264dp"
        android:layout_height="101dp"
        android:layout_marginTop="20dp"
        android:text="Lo primero que necesitas para comenzar a boxear. Pero debes saber que existen diferentes tipos de guantes."
        android:textColor="#000000"
        android:textColorHighlight="@color/black"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        tools:layout_editor_absoluteX="131dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="264dp"
        android:layout_height="101dp"
        android:layout_marginTop="12dp"
        android:text="Bajo los guantes se utilizan vendas para protejer las manos del roce continuo."
        android:textColor="#000000"
        android:textColorHighlight="@color/black"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_editor_absoluteX="131dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="264dp"
        android:layout_height="101dp"
        android:layout_marginTop="20dp"
        android:text="Los cascos principalmente indicados para cuando se vaya a practicar"
        android:textColor="#000000"
        android:textColorHighlight="@color/black"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        tools:layout_editor_absoluteX="131dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="90dp"
        android:layout_height="94dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toStartOf="@+id/textView15"
        app:srcCompat="@drawable/vendas_boxeo_everlast_negra_45_mts"
        tools:layout_editor_absoluteY="318dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Vendas de boxeo"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_editor_absoluteX="131dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="86dp"
        android:layout_height="91dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toStartOf="@+id/textView17"
        app:srcCompat="@drawable/_1_bdjm0r4l__sl500_"
        tools:layout_editor_absoluteY="503dp" />

    <include
        android:id="@+id/include"
        layout="@layout/activity_menus"
        android:layout_width="413dp"
        android:layout_height="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>